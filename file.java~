import java.io.*;
class file
{
    public static void main(String args[]) throws IOException
    {
        BufferedWriter bw=new BufferedWriter(new FileWriter("/mnt/c/Users/Amith/Desktop/amith.c"));
        BufferedReader br=new BufferedReader(new InputStreamReader(System.in));
        String str;
        str=br.readLine();
        bw.write(str+"\n");
        bw.close();
    }
}
